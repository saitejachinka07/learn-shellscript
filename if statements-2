job=$1
jenkins=$2

if [ -z "$job" ]; then
echo input is missing
exit
fi

if [ "$job" != step ]
then
     echo builds done and $jenkins is started

else [ "$job" == step ]
    echo -e builds done and "\e[36m $jenkins\e[0m " is broken    
fi

# number comparison

if [ "$job" -gt 100 ]
then 
  echo -e build done in "\e[36m $jenkins \e[0m" node

else 

     echo -e build done in "\e[36m savle \e[0m" node

fi


   

